<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/icon_labsce"
        android:label="@string/app_name"
        android:roundIcon="@color/black_special"
        android:supportsRtl="true"
        android:theme="@style/Theme.LabCE"
        tools:targetApi="31">
        <activity
            android:name=".ChangePwdActivity"
            android:exported="false" />
        <activity
            android:name=".TabScheduleActivity"
            android:exported="false" />
        <activity
            android:name=".ScheduleActivity"
            android:exported="false" />
        <activity
            android:name=".PrincipalActivity"
            android:exported="false"
            android:label="@string/title_activity_principal"
            android:theme="@style/Theme.LabCE.NoActionBar" />
        <activity
            android:name=".LogActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>